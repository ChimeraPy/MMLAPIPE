{
  "mode": "record",
  "workers": {
    "manager_ip": "10.66.191.251",
    "manager_port": 9001,
    "instances": [
      {
        "name": "local",
        "id": "local",
        "description": "local worker for the MMLA pipeline demo with a video node"
      }
    ]
  },
  "nodes": [
    {
      "registry_name": "MMLAPIPE_Video",
      "name": "screen-capture",
      "kwargs": {
        "video_src": "data/embodied/STEP-01-Run-Fish-screen.mp4",
        "width": 500,
        "height": 480,
        "frame_key": "frame",
        "include_meta": true
      },
    "package": "mmlapipe"
    },
    {
      "registry_name": "MMLAPIPE_Video",
      "name": "camera",
      "kwargs": {
        "video_src": "data/embodied/STEP-01-Run-Fish-front.mp4",
        "width": 500,
        "height": 480,
        "frame_key": "frame",
        "include_meta": true
      },
    "package": "mmlapipe"
    },
    {
      "registry_name": "MMLAPIPE_ShowWindows",
      "name": "show",
      "kwargs": {
        "window_xy": [
          300,
          300
        ],
        "items_per_row": 2
      },
      "package": "mmlapipe"
    }
  ],
  "adj": [
    [
      "screen-capture",
      "show"
    ],
    [
      "camera",
      "show"
    ]
  ],
  "manager_config": {
    "logdir": "cp-logs",
    "port": 9001
  },
  "mappings": {
    "local": [
      "screen-capture",
      "show",
      "camera"
    ]
  },
  "timeouts": {
    "commit_timeout": 120,
    "shutdown_timeout": 300
  }
}
