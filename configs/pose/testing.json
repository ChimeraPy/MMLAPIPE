{
    "mode": "record",
    "workers": {
      "manager_ip": "129.59.104.153",
      "manager_port": 9001,
      "instances": [
        {
          "name": "local",
          "id": "local",
          "description": "local worker for the MMLA pipeline demo with a video node"
        }

      ]
    },
    "nodes": [
      {
        "registry_name": "CPPipelines_HFVideo",
        "name": "vid111",
        "kwargs": {
          "video_src": "./vid.mp4",
          "width": 640,
          "height": 480,
          "frame_key": "frame",
          "frame_rate": 30,
          "include_meta": true,
          "loop": true
        },
        "package": "chimerapy-pipelines"
      },
     
      {
        "registry_name": "CPPipelines_HFNode",
        "name": "pose",
        "kwargs": {
          "model_name": "hustvl/yolos-tiny"
        },
        
        "package": "chimerapy-pipelines"
      },

      {
        "registry_name": "CPPipelines_HFDisplay",
        "name": "display",
        "kwargs": {
          "frames_key": "frame"
        },
        "package": "chimerapy-pipelines"
      }
    ],
    "adj": [
      [
        "vid111",
        "pose"
      ],
      [
        "pose",
        "display"
      ]
    ],
    "manager_config": {
      "logdir": "cp-logs",
      "port": 9001
    },
    "mappings": {
      "local": [
        "vid111",
        "display",
        "pose"
      ]
    },
    "timeouts": {
      "commit_timeout": 500,
      "shutdown_timeout": 300
    }
  }
  